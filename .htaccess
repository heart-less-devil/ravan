# Minimal .htaccess for testing
RewriteEngine On

# Allow all files
Options +FollowSymLinks

# Basic SPA routing
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /index.html [QSA,L]

# Allow access to all file types
<FilesMatch ".*">
    Order Allow,Deny
    Allow from all
</FilesMatch>